{
  "name": "maravian-playwright-llm-scraper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "build": "tsc && node -e \"const fs=require('fs');const path=require('path');const dir='dist/static';if(!fs.existsSync(dir)){fs.mkdirSync(dir,{recursive:true})}fs.cpSync('src/static', 'dist/static', {recursive: true})\"",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "https-proxy-agent": "^7.0.6",
    "openai": "^4.96.0",
    "playwright": "^1.52.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.8.3"
  }
}
